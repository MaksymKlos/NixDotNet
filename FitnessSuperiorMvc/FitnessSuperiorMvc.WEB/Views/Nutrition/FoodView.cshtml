@model FitnessSuperiorMvc.DA.Entities.Nutrition.Food
<div class="main__content reg-content m-height">
    <div class="models">
        <h1 class="model-title center">@Model.Name</h1>
        <p class="model-text center"><span class="model-span"> Calories:</span> @Model.Calories</p>
        <p class="model-text center"><span class="model-span"> Proteins:</span> @Model.Proteins</p>
        <p class="model-text center"><span class="model-span"> Fats:</span> @Model.Fats</p>
        <p class="model-text center"><span class="model-span"> Carbohydrates:</span> @Model.Carbohydrates</p>
        <div class="d-flex justify-content-end">
            <a class="grey-btn" href="@ViewBag.ReturnUrl">Get back</a>
        </div>
    </div>
    @section Styles{
        <link href="~/css/model.css" type="text/css" rel="stylesheet" />
    }
</div>